{
	"name": "morejs",
	"displayName": "morejs",
	"description": "Outliner Extension for vscode",
	"keywords": [
		"Outliner",
		"Outline",
		"Editor",
		"Vscode"
	],
	"author": "FÃ©lix",
	"license": "ISC",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "morejs.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "morejs.selectNode",
				"title": "Select Node"
			},
			{
				"command": "morejs.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "morejs.refreshNode",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "morejs.renameNode",
				"title": "Rename"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "jsonOutline",
					"name": "Json Outline",
					"when": "jsonOutlineEnabled"
				},
				{
					"id": "jsOutline",
					"name": "More Outline",
					"when": "jsOutlineEnabled"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "jsonOutline",
				"contents": "Select a JSON file to view the outline and edit node labels"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "morejs.helloWorld"
				}
			],
			"view/title": [
				{
					"command": "morejs.refresh",
					"when": "view == jsonOutline",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "morejs.renameNode",
					"when": "view == jsonOutline"
				},
				{
					"command": "morejs.refreshNode",
					"when": "view == jsonOutline",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2"
	}
}